&i2c19 {
	status = "okay";
	goodix@5d {
		compatible = "goodix,gt9889";
		reg = <0x5d>;
		status = "okay";
		interrupt-parent = <&tlmm>;
		interrupts = <122 0x2800>;
		vtouch-load = <600000>;
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&tp_active>;
		pinctrl-1 = <&tp_int_suspend &tp_reset_suspend>;
		goodix,reset-gpio = <&tlmm 54 0x0>;
		goodix,irq-gpio = <&tlmm 122 0x2800>;
		goodix,vdd-gpio = <&tlmm 98 0x0>;			/* 3v3 */
		goodix,avdd-gpio = <&tlmm 100 0x0>;			/* 1v8 */
		goodix,irq-flags = <2>; /* 1:trigger rising, 2:trigger falling;*/
		goodix,panel-max-id = <10>;
		goodix,panel-max-x = <1079>;
		goodix,panel-max-y = <2339>;
		goodix,panel-max-w = <127>;
		goodix,power-on-delay-us = <300000>; /*300ms*/
		goodix,power-off-delay-us = <5000>;
	};
};

&ipa {
	status = "okay";
	memory-region = <&ipa_fw_mem>;
	firmware-name = "qcom/sm8150/Xiaomi/raphael/ipa_fws.mbn";
};

&gpi_dma0 {
	status = "okay";
};

&gpi_dma1 {
	status = "okay";
};

&gpi_dma2 {
	status = "okay";
};

&gmu {
	status = "okay";
};

&gpu {
	status = "okay";
	
	zap-shader {
		memory-region = <&gpu_mem>;
		firmware-name = "qcom/sm8150/Xiaomi/raphael/a640_zap.mbn";
	};
};

&mdss_dsi0 {
	vdda-supply = <&vdda_mipi_dsi0_1p2>;
	status = "okay";
	panel:	panel@0 {
		compatible = "samsung,ams639rq08";
		reg = <0>;
		status = "okay";
		
		vddio-supply = <&disp_vci_vreg>;
		vdd3p3-supply = <&vreg_l13a_2p7>;

		te-gpios = <&tlmm 8 GPIO_ACTIVE_LOW>;
		reset-gpios = <&tlmm 6 GPIO_ACTIVE_LOW>;
		
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&panel_default>;
		pinctrl-1 = <&panel_sleep>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mdss_dsi0_out>;
			};
		};
	};
};

&mdss_dsi0_out {
	data-lanes = <0 1 2 3>;
	remote-endpoint = <&panel_in>;
};

&mdss_dsi0_phy {
	status = "okay";
	vdds-supply = <&vreg_l5a_0p875>;
};

&mdss {
	status = "okay";
};

&pm8150_gpios {
	vol_up_n: vol-up-n-state {
		pins = "gpio6";
		function = PMIC_GPIO_FUNC_NORMAL;
		input-enable;
		bias-pull-up;
	};
};

&pon {
	mode-bootloader = <0x2>;
	mode-recovery = <0x1>;
};

&pon_pwrkey {
	status = "okay";
};

&pon_resin {
	status = "okay";

	linux,code = <KEY_VOLUMEDOWN>;
};

&qupv3_id_1 {
	status = "okay";
};

&remoteproc_adsp {
	status = "okay";
	firmware-name = "qcom/sm8150/Xiaomi/raphael/adsp.mbn";
};

&remoteproc_cdsp {
	status = "okay";
	firmware-name = "qcom/sm8150/Xiaomi/raphael/cdsp.mbn";
};

&remoteproc_mpss {
	status = "okay";
	firmware-name = "qcom/sm8150/Xiaomi/raphael/modem.mbn";
};

&remoteproc_slpi {
	status = "disabled";
	firmware-name = "qcom/sm8150/Xiaomi/raphael/slpi.mbn";
};

&tlmm {
	/* gpios 0..3 are NFC spi, gpios 126..129 are FP spi */
	gpio-reserved-ranges = <0 4>, <126 4>;

	/* GPIOs for QUP UART */
	qup_uart13_sleep: qup-uart13-sleep {
		pinmux {
			pins = "gpio43", "gpio44", "gpio46";
			function = "qup13";
		};

		pinconf-cts-rx {
			pins = "gpio43", "gpio46";
			drive-strength = <2>;
			bias-disable;
		};

		pinconf-rts {
			pins = "gpio44";
			drive-strength = <2>;
			bias-pull-down;
		};

		pinconf-tx {
			pins = "gpio45";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	/* GPIOs for display panel */
	panel_default: panel-default-state {
		te-pins {
			pins = "gpio8";
			function = "mdp_vsync";
			drive-strength = <2>;
			bias-pull-down;
		};

		reset-pins {
			pins = "gpio6";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
		};
	};

	panel_sleep: panel-sleep-state {
		te-pins {
			pins = "gpio8";
			function = "mdp_vsync";
			drive-strength = <2>;
			bias-pull-down;
		};

		reset-pins {
			pins = "gpio6";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
		};
	};

	/* GPIOs for touch panel */
	tp_active: tp_active {
			pins = "gpio122", "gpio54";
			function = "gpio";
			drive-strength = <16>;
			bias-pull-up;
	};
	
	tp_int_suspend: tp_int_suspend {
			pins = "gpio122";
			function = "gpio";
			drive-strength = <16>;
			input-enable;
			slew-rate = <0>;
			bias-pull-up;
	};

	tp_reset_suspend: tp_reset_suspend {
			pins = "gpio54";
			function = "gpio";
			drive-strength = <16>;
			slew-rate = <1>;
			bias-disable;
			output-high;
	};
};

&uart2 {
	status = "okay";
};

